{
  "name": "e-commerce-site",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start:open-ssl": "ng serve --ssl --open --port=5200 --project=ui",
    "start:ssl": "ng serve --ssl --port=5200 --project=ui",
    "start": "ng serve --port=5200 --project=ui",
    "build": "ng build",
    "clean": "node ./node_modules/rimraf/rimraf.js -- wwwroot",
    "build:prod": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng build --stats-json --source-map --aot=true ui",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve:ssr": "npm run build:prod && ng run ui:server && node dist/ui/server/main.js",
    "build:ssr": "ng build && npm run serve:ssr",
    "prerender": "ng run ui:prerender",
    "pretty-quick": "pretty-quick",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/platform-server": "^18.2.0",
    "@angular/router": "^18.2.0",
    "express": "^4.15.2",
    "file-saver": "^2.0.5",
    "hammerjs": "^2.0.8",
    "ngx-device-detector": "^7.0.0",
    "ngx-image2dataurl": "^15.0.0",
    "ngx-toastr": "^17.0.2",
    "rxjs": "^6.5.5",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.0",
    "@angular/cli": "^18.2.0",
    "@angular/compiler-cli": "^18.2.0",
    "@angular/elements": "^18.2.0",
    "@angular/language-service": "^18.2.0",
    "@babel/core": "^7.14.6",
    "@compodoc/compodoc": "^1.1.11",
    "@types/express": "^4.17.6",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.47",
    "@webcomponents/custom-elements": "^1.4.3",
    "babel-loader": "^8.2.2",
    "browser-sync": "^3.0.0",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.5.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^18.2.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "storybook": "^7.0.25",
    "tslint": "~6.1.0",
    "typescript": "~5.4.5"
  }
}
